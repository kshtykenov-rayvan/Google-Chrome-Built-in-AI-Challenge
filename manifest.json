{
  "manifest_version": 3,
  "name": "My Chrome Extension with Side Panel",
  "version": "1.0",
  "description": "Расширение с боковой панелью",
  "permissions": [],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/free-icon-hand-128.png",
      "48": "icons/free-icon-hand-128.png",
      "128": "icons/free-icon-hand-128.png"
    }
  },
  "icons": {
    "16": "icons/free-icon-hand-128.png",
    "48": "icons/free-icon-hand-128.png",
    "128": "icons/free-icon-hand-128.png"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  }
}
